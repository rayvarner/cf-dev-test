component displayname="Speaker" extends="BaseBean" accessors="true" {	property name="speakerID" type="numeric" default="0" primary=true;	property name="speakerUid" type="string" default="00000000-0000-0000-0000-000000000000";	property name="speakerTypeID" type="any" default="";	property name="firstName" type="string" default="";	property name="lastName" type="string" default="";	property name="jobTitle" type="string" default="";	property name="company" type="string" default="";	property name="email" type="string" default="";	property name="bio" type="string" default="";	property name="hideSpeaker" type="boolean" default="false";	property name="createdOn" type="date" default="2000-01-01 00:01:00";	property name="updatedOn" type="date" default="2000-01-01 00:01:00";	property name="isActive" type="boolean" default="true";	public string function getFullName() {		return getFirstName() & ' ' & getLastName();	}	public struct function getMemento() {		var _Speaker = {};		_Speaker[ 'speakerID' ] = getSpeakerID();		_Speaker[ 'speakerUid' ] = getSpeakerUid();		_Speaker[ 'speakerTypeID' ] = getSpeakerTypeID();		_Speaker[ 'firstName' ] = getFirstName();		_Speaker[ 'lastName' ] = getLastName();		_Speaker[ 'jobTitle' ] = getJobTitle();		_Speaker[ 'company' ] = getCompany();		_Speaker[ 'email' ] = getEmail();		_Speaker[ 'bio' ] = getBio();		_Speaker[ 'hideSpeaker' ] = getHideSpeaker();		_Speaker[ 'createdOn' ] = getCreatedOn();		_Speaker[ 'updatedOn' ] = getUpdatedOn();		_Speaker[ 'isActive' ] = getIsActive();		return _Speaker;	}}